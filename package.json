    "kafkajs": "^2.2.0",
    "moment-timezone": "^0.5.37",
    "mongoose": "^6.6.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^9.0.0"
